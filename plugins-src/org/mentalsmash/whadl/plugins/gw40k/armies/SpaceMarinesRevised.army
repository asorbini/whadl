	unit Captain {
		cost 100;
		composition Captain;
		slots * HQ;
		member Captain {
			extends ChapterMaster.ChapterMaster;
		}
		
		special {
			unit CommandSquad {
				slots FREE;
			}
			
			unit SpaceMarineBikeSquad {
				slots * TROOP;
				
				
				conditions (
					this->slots contains {TROOP}?
						this->size >= 5:
						true
				);
			}
			
			conditions (
				(this->units select (
					this->type == SpaceMarineBikeSquad &&
					this->slots contains {TROOP}
				))->size > 0 ?
				
					(
						(this->units select (
							this->type == Captain &&
							this->members select (
								this->equipment contains {SpaceMarineBike}
							)
						))->size > 0
						
					)->size > 0
					
					: true
			);
		}
	}
	
