	unit TerminatorSquad {
		cost 200;
		composition (TerminatorSergeant,
						4 Terminator, [5 Terminator for 40]);
		linked ([LandRaider or LandRaiderRedeemer or LandRaiderCrusader]);
		slots * ELITE;
		
		member Terminator {
			equipment (
				TerminatorArmour,
				StormBolter or
				HeavyFlamer for 5 or
				AssaultCannon for 30,
				[CycloneMissileLauncher for 30],
				PowerFist or
				ChainFist for 5
			);
		}
		
		member TerminatorSergeant {
			equipment (
				TerminatorArmour,
				StormBolter,
				PowerWeapon
			);
		}
		
		
		conditions(
			/**
			 * Only 1 every 5 members can select one
			 * of the heavy weapons
			 **/
			(this->members select (
				(this->equipment intersect {
					HeavyFlamer,
					AssaultCannon,
					CycloneMissileLauncher
				})->size > 0
			))->size <= (this->members->size / 5)
		&&
			/**
			 * A member can't select a heavy weapon and
			 * still take the additional CycloneMissileLauncher
			 **/
			this->members each (
				(this->equipment intersect {
					HeavyFlamer,
					AssaultCannon,
					CycloneMissileLauncher
				})->size < 2
			)
		);
	}
	
	
