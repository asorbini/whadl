	unit TacticalSquad {
		cost 90;
		composition (SpaceMarineSergeant, 4 SpaceMarine, [5 SpaceMarine for 16]);
		linked ([Rhino or Razorback or DropPod]);
		slots * TROOP;
		member SpaceMarine {
			equipment (
			    PowerArmour,
				BoltPistol,
				(Boltgun or
				HeavyBolter for 0 or
				MultiMelta for 0 or
				MissileLauncher for 0 or
				PlasmaCannon for 5 or
				Lascannon for 10 or
				Flamer for 0 or
				Meltagun for 5 or
				PlasmaGun for 10),
				FragGrenades,
				KrakGrenades);
		}
		member SpaceMarineSergeant {
			equipment ((BoltPistol or
					   Chainsword for 0 or
					   CombiFlamer for 10 or
					   CombiPlasma for 10 or
					   CombiMelta for 10 or
					   PlasmaPistol for 15 or
					   PowerWeapon for 15 or
					   PowerFist for 25),
					   (Boltgun or
					   Chainsword for 0 or
					   CombiFlamer for 10 or
					   CombiPlasma for 10 or
					   CombiMelta for 10 or
					   PlasmaPistol for 15 or
					   PowerWeapon for 15 or
					   PowerFist for 25),
					   PowerArmour,
					   FragGrenades,
					   KrakGrenades,
					   [MeltaBombs for 5],
					   [TeleportHomer for 5]);
		}
		
		conditions (
			(
				/**
				 * if one of the squad's members is equipped with an
				 * heavy weapon then there must be only one such member and
				 * the squad must be composed of 10 members.
				 **/
				(this->members select (
					(this->equipment intersect {
							Lascannon,
							MissileLauncher,
							PlasmaCannon,
							HeavyBolter,
							MultiMelta
					})->size > 0
				))->size > 0
				
				?
					
				(this->members select (
					(this->equipment intersect {
							Lascannon,
							MissileLauncher,
							PlasmaCannon,
							HeavyBolter,
							MultiMelta
					})->size > 0
				))->size == 1 &&
				
				(this->members->size == 10)
				
				:
				
				true
				
			) && (
				/**
				 * if one of the squad's members is equipped with a
				 * special weapon then there must be only one such member and
				 * the squad must be composed of 10 members.
				 **/
				(this->members select (
					(this->equipment intersect {
							Flamer,
							Meltagun,
							PlasmaGun
					})->size > 0
				))->size > 0
				
				?
					
				(this->members select (
					(this->equipment intersect {
							Flamer,
							Meltagun,
							PlasmaGun
					})->size > 0
				))->size == 1 &&
				
				(this->members->size == 10)
					
				:
				
				true
			)
		);
		
		special {
			unit Rhino {
				slots 1 TRANSPORT;
			}
			
			unit Razorback {
				slots 1 TRANSPORT;
			}
			
			unit DropPod {
				slots 1 TRANSPORT;
			}
		}
	}
	
