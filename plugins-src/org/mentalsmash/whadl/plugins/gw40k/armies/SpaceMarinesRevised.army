	conditions (
	
		#CONDITIONS
		 
		(	/**
			 * ONLY ONE UNIT CAN TAKE SergeantTelion
			 **/
			(this->units select (
				(this->members select (
					this->type == ScoutSquad.SergeantTelion
				))->size > 0
			))->size <= 1
		&&
			/**
			 * ONLY ONE UNIT CAN TAKE SergeantCronus
			 **/
			(this->units select (
				this->upgrades contains {SergeantCronus}
			))->size <= 1
		&&	
			/**
			 * ONLY ONE TerminatoAssaultSquad CAN SELECT
			 * A LandRaider* AS TRANSPORT
			 **/
			(this->units select (
				this->type == TerminatorAssaultSquad &&
				(this->linked select (
					this->type == LandRaider || 
					this->type == LandRaiderRedeemer || 
					this->type == LandRaiderCrusader
				))->size > 0
			))->size <= 1
		&&
			/**
			 * ONLY ONE TerminatoSquad CAN SELECT
			 * A LandRaider* AS TRANSPORT
			 **/
			(this->units select (
				this->type == TerminatorSquad &&
				(this->members select (
					this->type == LandRaider || 
					this->type == LandRaiderRedeemer || 
					this->type == LandRaiderCrusader
